{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "lint-staged": "^6.0.0",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "git add . && lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write --no-bracket-spacing --jsx-bracket-same-line --no-semi --trailing-comma all --single-quote",
        "git add"
      ]
    }
  }
}
